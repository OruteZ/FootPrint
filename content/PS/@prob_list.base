formulas:
  Tier: ""
views:
  - type: table
    name: Table
    filters:
      and:
        - file.folder.endsWith("PS")
        - file.hasTag("problem_solving")
    order:
      - file.name
      - prob_id
      - prob_name
      - created_date
      - tier
      - formula.Tier
    sort:
      - property: tier
        direction: ASC
      - property: created_date
        direction: DESC
    limit: 10
    columnSize:
      note.prob_name: 217
